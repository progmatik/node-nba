{
  "name": "nba",
  "version": "1.0.0",
  "description": "node nba api stats.nba.com",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --require babel-register --recursive --timeout 6000 --exit",
    "coverage": "cross-env NODE_ENV=test nyc --all --reporter=text --reporter=lcov mocha test/*.js",
    "coverage-only": "cross-env NODE_ENV=test nyc --all --reporter=text mocha test/**/*.js --reporter=lcov --no-colors -R mocha-silent-reporter --recursive test/",
    "babel": "babel --presets es2015 js/main.js -o build/main.bundle.js",
    "build": "rm -rf ./dist; babel ./ --out-dir dist --ignore ./.babelrc,./vue,./node_modules,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && nodemon ./dist/server2.js",
    "webpack": "webpack",
    "lint": "./node_modules/.bin/eslint --fix src/**/*.js"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "build",
      "test/**/*.js",
      "config",
      "*.js",
      "coverage",
      "dist",
      "src/vendor/*.js"
    ],
    "reporter": [],
    "sourceMap": false,
    "instrument": true,
    "cache": false,
    "all": false
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.46",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "connect": "^3.6.6",
    "fs": "0.0.1-security",
    "got": "^8.3.1",
    "https": "^1.0.0",
    "node-fetch": "^2.1.2",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "require": "^2.4.20",
    "rimraf": "^2.6.2",
    "serve-static": "^1.13.2",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "lint-staged": "^6.1.0",
    "mocha": "^4.0.1",
    "mocha-better-spec-reporter": "^3.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-silent-reporter": "^1.0.0",
    "nyc": "^11.3.0",
    "prettier": "^1.10.2",
    "dirty-chai": "^2.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "json-loader": "^0.5.7",
    "source-map-support": "^0.5.5",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "babel-eslint": "^7.2.3",
    "webpack-node-externals": "^1.7.2"
  }
